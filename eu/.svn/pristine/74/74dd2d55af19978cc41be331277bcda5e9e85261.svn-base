@charset "utf-8";
/* CSS Document */

.container-fluid {
	width: 100%;
	height:auto;
	margin:0 auto;
	min-width:1200px;
	background-color:#f1f1f1;
}
select{height:50px; line-height:18px; padding:2px 0}
img{
	width:100%;
	height:100%;
}
.header {
	width: 100%;
	height:50px;
	background-color:#0376AD;
	min-width: 1000px;
	float: left;
	display: block;
	z-index: 100;
	overflow: visible;
}
.header_input{
	width:150px;
	height:100%;
	float:left;
	line-height:50px;
	text-align:center;
	font-size:14px;
	color:#FFF;
	margin-left:50px;
}
.header_input a{
	color:#FFF;
}
.header_list{
	width:100%;
	margin:0 auto;
	background-color:#f1f1f1;
	color:#000;
}
.header_list_One{
	width:100%;
	height:110px;
	border:1px solid red;
	background-color:#FFF;
}
.but{
	width:10%;
	height: 33px;
	margin-top:10px;
	float:right;
	border:none;
	  border-radius:5px;
	-moz-border-radius:5px;
	-webkit-border-radius:5px;
	margin-left:5px;
	color:#FFF;
	background: #E74B3C url(../images/icon_fb.png) no-repeat;
	background-position:left;
	background-position:5px;
	padding-left:20px;
	margin-right:100px;

}
.butt{
	float:left;
	 width:100px;
	 height:35px;
	  margin-top:9px; 
	  background-color:#005983;
